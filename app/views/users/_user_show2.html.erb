<h2><%= @user.name %>'s Posts:</h2>
<ul>
    <% @user.posts.each do |post| %>

    <li><%= link_to(post.content, post_path(post)) %></li>

<li><% if post.image %><%= image_tag(post.image) %><% end %></li>
<li><% if post.emoticon %><%= post.emoticon.image %><% end %></li>
<li><% if post.user %><%= link_to(post.user.name, user_path(post.user)) %><% end %></li>

    <% end %>
</ul>

<nav aria-label="post display pages">
  <ul class="pagination justify-content-center" style="margin:20px 0">
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
      </a>
    </li>
    <li class="page-item"><a class="page-link" href="#">1</a></li>
    <li class="page-item"><a class="page-link" href="#">2</a></li>
    <li class="page-item"><a class="page-link" href="#">3</a></li>
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
      </a>
    </li>
  </ul>
</nav>